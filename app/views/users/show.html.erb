<h1>Welcome <%= @user.name %></h1>
<h4>Username: (<%= @user.username %>)</h4>
<%= form_tag(packs_path, :method => "get", id: "search-form") do %>
    <%= text_field_tag :search, params[:search], placeholder: "Search Packs" %>
    <%= submit_tag "Search", :name => nil %>
<% end %>

<h2>My Packs</h2>
  <ul>
    <% @packs.each do |pack| %>
      <li><%= link_to pack.name, pack_path(pack) %></li>
    <% end %>
  </ul>

  <a name="new">
    <h2>Create A New Pack</h2>
    <%= form_for @pack do |f| %>
      <%= f.label :location_name %>
      <%= f.text_field :location_name %>
      <%= f.submit %>
    <% end %>
  </div>
  <h2>Need examples?<%= link_to "Look at previous packs", packs_path%><br>



  <button type="button" name="button">
    <%= link_to("Log Out", logout_path, method: :delete) %>
  </button>
